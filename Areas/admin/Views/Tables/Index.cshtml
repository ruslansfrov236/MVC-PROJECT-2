
@model Statistics
<div class="row mt-3">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">Basic Table</h5>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">#</th>
                                <th scope="col">Stastics</th>
                                <th scope="col">Name </th>
                                <th scope="col">Duration</th>
                                <th scope="col">Edit</th>
                                <th scope="col">Delete</th>
                                <th scope="col">Details</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th scope="row">1</th>
                                <td>@Model.Statistic</td>
                                <td>@Model.StatisticsName</td>
                                <td>@Model.StatisticsDuration</td>
                                <td> <a href="update/@Model.Id">Edit</a></td>
                                <td> <a href="form">Create</a></td>
                                <td> <a href="delete/@Model.Id">Delete</a></td>
                             
                            </tr>
                            
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>